-- Draggable
task.spawn(function()
	local script = draggable

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	Background = script.Parent.Background
	Background.Draggable = true
	Background.Selectable = true
	Background.Active = true
end)

-- RainbowEffect
task.spawn(function()
	local script = rainbow_effect_4

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	while true do
		for a = 1,360,1 do
			wait(0.1)
			script.Parent.Color = Color3.fromHSV(a/360,1,1)
		end
	end
end)

-- GuiToggleFunction
