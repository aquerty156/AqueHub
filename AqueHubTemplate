-- Keys
task.spawn(function()
	local script = keys

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local Prefix = "p"
	local Prefix2 = "m"
	local Background = script.Parent.Background
	local GuiToggle = script.Parent.GuiToggle

	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(Key)
		if Key == Prefix then
			if Prefix ~= "/" then
				if Background.Visible == true then
					Background.Visible = false
					GuiToggle.Visible = true
				else
					Background.Visible = true
					GuiToggle.Visible = false
				end
			end
		end
	end)

	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(Key)
		if Key == Prefix2 then
			if Prefix2 ~= "/" then
				Background.Position = UDim2.new(0.559176207, 0, 0.013049975, 0)
			end
		end
	end)
end)
