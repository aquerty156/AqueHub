--//Modules

local modules = {}

--// Scripts

-- RainbowEffect
task.spawn(function()
	local script = rainbow_effect

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	while true do
		for a = 1,360,1 do
			wait(0.1)
			script.Parent.Color = Color3.fromHSV(a/360,1,1)
		end
	end
end)

-- CloseButtonFunction
task.spawn(function()
	local script = close_button_function

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	CloseButton = script.Parent
	Gui = script.Parent.Parent.Parent.Parent

	local BackgroundTween = script.Parent.Parent.Parent.BackgroundTween
	local Background = Gui.Background
	local BackgroundStroke = Background.BackgroundStroke
	local BackgroundHolder = Background.BackgroundHolder

	local InsideBackground = {
		Background.BackgroundTween,
		Background.SectionsHolder,
		Background.SectionsDefault,
		Background.ScriptsHolder,
		Background.TitleFrame
	}

	local CloseProgress = false
	CloseButton.MouseButton1Click:Connect(function()
		if CloseProgress == false then
			CloseProgress = true
			local TweenService = game:GetService("TweenService")
			local TweenInfo = TweenInfo.new(0.4)
			local Target = {BackgroundTransparency = 0}
			local Tween1 = TweenService:Create(BackgroundTween, TweenInfo, Target)
			Tween1:Play()
			wait(.4)
			for i,v in pairs(InsideBackground)  do
				v.Visible = false
			end
			BackgroundHolder:TweenSizeAndPosition(
				UDim2.new(0, 0, 0, 0),
				UDim2.new(0.5, 0, 0.5, 0),
				Enum.EasingDirection.Out,
				Enum.EasingStyle.Sine,
				0.6,
				true)
			BackgroundStroke:TweenSizeAndPosition(
				UDim2.new(0, 0, 0, 0),
				UDim2.new(0.5, 0, 0.5, 0),
				Enum.EasingDirection.Out,
				Enum.EasingStyle.Sine,
				0.6,
				true)
			wait(.6)
			Gui:Destroy()
		end
	end)
end)

-- HelpButtonFunction
task.spawn(function()
	local script = help_button_function

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	HelpButton = script.Parent
	local GUI = script.Parent.Parent.Parent.Parent
	local HelpFrame = GUI.HelpFrame

	HelpButton.MouseButton1Click:Connect(function()
		HelpFrame.Visible = true
	end)
end)

