-- SectionFunction
task.spawn(function()
	local script = section_function

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local LocalSection = script.Parent
	local Background = LocalSection.Parent.Parent
	local TweenService = game:GetService("TweenService")
	local SectionsHolder = Background.SectionsHolder
	local ScriptsHolder = Background.ScriptsHolder
	local SectionsDefault = Background.SectionsDefault

	function GetSection()
		local parent = script.Parent
		local name = parent.Name
		local lastDigit = string.match(name, "%d$")
		return tonumber(lastDigit)
	end

	local LocalNumber = GetSection()

	local section1 = SectionsHolder["Section 1"]
	local section2 = SectionsHolder["Section 2"]
	local section3 = SectionsHolder["Section 3"]
	local section4 = SectionsHolder["Section 4"]
	local section5 = SectionsHolder["Section 5"]
	local section6 = SectionsHolder["Section 6"]

	local Script1 = ScriptsHolder.Script1
	local Script2 = ScriptsHolder.Script2
	local Script3 = ScriptsHolder.Script3
	local Script4 = ScriptsHolder.Script4
	local Script5 = ScriptsHolder.Script5
	local Script6 = ScriptsHolder.Script6

	local Default1 = SectionsDefault.Default1
	local Default2 = SectionsDefault.Default2
	local Default3 = SectionsDefault.Default3
	local Default4 = SectionsDefault.Default4
	local Default5 = SectionsDefault.Default5
	local Default6 = SectionsDefault.Default6

	local Sections = {
		section1,
		section2,
		section3,
		section4,
		section5,
		section6
	}
	local Scripts = {
		Script1,
		Script2,
		Script3,
		Script4,
		Script5,
		Script6
	}
	local Defaults = {
		Default1,
		Default2,
		Default3,
		Default4,
		Default5,
		Default6
	}

	InProgress = false
	LocalSection.MouseButton1Click:Connect(function()
		for i,v in pairs(Sections) do
			v.BackgroundTransparency = 0
		end
		for i,v in pairs(Sections) do
			v.TextColor3 = Color3.fromRGB(255, 255, 255)
		end
		for i,v in pairs(Scripts) do
			v.Visible = false
		end
		for i,v in pairs(Defaults) do
			v.BackgroundTransparency = 0
		end
		Sections[LocalNumber].BackgroundTransparency = 1
		Sections[LocalNumber].TextColor3 = Color3.fromRGB(0, 0, 0)
		Scripts[LocalNumber].Visible = true
		Defaults[LocalNumber].BackgroundTransparency = 1
	end)
end)

